config {
  type: 'view',
  description: 'The table of courier motivations',
  schema: 'staging',
  columns: table_docs.stg_courier__orders,
  tags: ['courier', 'staging', 'orders', 'view'],
  name:'stg_courier__orders',
  assertions: {
    nonNull: ['id'],
    uniqueKey: ['id']
  }
}



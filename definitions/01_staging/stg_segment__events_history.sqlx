config {
  type: 'view',
  description: 'Getting historical Segment events',
  schema: 'staging',
  columns: table_docs.stg_courier__orders,
  tags: ['segment', 'staging'],
  name:'stg_segment__events_history',
}


select
  *
from
  ${ref('segment_events')}
where
  timestamp between timestamp_sub('2025-11-15', interval 7 day) and '2025-11-15'